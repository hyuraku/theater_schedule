.col-md-12
  .row.result__row
    .col-md-1
    .col-md-10.text-primary
      .list-group
        .result__first{ 'v-show': 'scheduleTopBool'}
        .list-group-item.result__one{ 'v-show': 'scheduleInfoBool','v-for': 'schedule in scheduleInfo' }
          .row
            .col-md-2.col-xs-6.result__one__border
              {{schedule.schedule_day}}
            .col-md-4.col-xs-6.result__one__border
              {{schedule.theater_name}}
            .col-md-6.col-xs-12.result__one__border
              {{schedule.work_name}}
          .row
            .col-md-2.col-xs-6.result__one__border
              {{schedule.screen_name}}
            .col-md-2.col-xs-6.result__one__border
              {{schedule.work_minutes}}
            .col-md-4.col-xs-6.result__one__border
              {{schedule.start_time}}
            .col-md-4.col-xs-6.result__one__border
              {{schedule.end_time}}
    .col-md-1
