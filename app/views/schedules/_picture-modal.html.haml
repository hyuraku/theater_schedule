.modal.fade#picture-modal{ "aria-hidden": "true", "aria-labelledby": "picture-modal-label"}
  .modal-dialog
    .modal-content
      .modal-header
        %h5.modal-title#picture-modal-label Pictures list
        %button.close{ "aria-label": "Close", "data-dismiss": "modal", type: "button"}
          %span{"aria-hidden": "true"} Ã—
      .modal-body
        - @works.each do |work|
          %tr
            %td= simple_format(work)
      .modal-footer
        %button.btn.btn-secondary{ "data-dismiss": "modal", type: "button"} Close
        %button.btn.btn-primary{type: "button"} Search
